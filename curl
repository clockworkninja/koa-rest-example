//LOGIN USER
curl -d '{"username": "user", "password": "secret"}' -H "Content-Type: application/json"  -X POST http://localhost:3000/login -i

//LOGIN ADMIN
curl -d '{"username": "admin", "password": "secret"}' -H "Content-Type: application/json"  -X POST http://localhost:3000/login -i 

//POST MESSAGE
curl -d '{"from": "user", "to": "admin" , "message":"this is a message"}' -H "Content-Type: application/json" -X POST http://localhost:3000/message -i -H "KoaToken: <TOKEN_HERE>"

//GET STATS
curl http://localhost:3000/stats -i -H "KoaToken: <TOKEN_HERE>"


//LOGOUT
curl http://localhost:3000/logout -i -H "KoaToken: <TOKEN_HERE>"

